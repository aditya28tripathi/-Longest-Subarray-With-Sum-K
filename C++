int longestSubarrayWithSumK(vector<int> a, long long k) {
    // Write your code here
int n = a.size();
    int count = 0;

    for(int i =0;i<n;i++){
            long long s = 0;
        for(int j =i;j<n;j++){

            s = s+ a[j];

            if(s==k)
        count = max(count  , j-i+1);


        }
    }

    return count;
}
